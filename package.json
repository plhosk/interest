  "scripts": {
    "build-prod": "babel server -d server-compiled --presets=es2015,stage-0 && cross-env NODE_ENV=production webpack --config ./webpack.prod.js -p --progress",
    "start": "node ./server/register.js",
    "start-prod": "cross-env NODE_ENV=production node ./server-compiled/server.js",
    "remove-source-map": "rm ./public/bundle.js.map",
    "remove-static": "rm -rf ./public/*.js ./public/index.html ./server-compiled/",
    "heroku-start": "cross-env NODE_ENV=production node server-compiled/server.js",
    "heroku-postbuild": "babel server -d server-compiled --presets=es2015,stage-0 && cross-env NODE_ENV=production webpack --config ./webpack.prod.js -p"
  },
